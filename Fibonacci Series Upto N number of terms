import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
		int a,b,c,i,n;
		a=1;
		b=1;
	    System.out.print("Enter the number of terms ");
	    Scanner s = new Scanner(System.in);
		n= s.nextInt();
		System.out.println(a);
		System.out.println(b);
		for(i=2;i<n;i++)
		{
		    c=a+b;
		    System.out.println(c);
		    a=b;
		    b=c;
		}
	}
}
